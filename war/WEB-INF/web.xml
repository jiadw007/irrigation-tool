<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>calculateAction</servlet-name>
		<servlet-class>com.project.server.calculateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>controller</servlet-name>
		<servlet-class>com.project.server.controllerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>calculateAction</servlet-name>
		<url-pattern>/calculate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>loginAction</servlet-name>
		<servlet-class>com.project.server.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginAction</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logoutAction</servlet-name>
		<servlet-class>com.project.server.logoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutAction</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	
		<servlet-name>controller</servlet-name>
		<url-pattern>/weeklyReport</url-pattern>
	
	</servlet-mapping>
	<servlet-mapping>
	
		<servlet-name>controller</servlet-name>
		<url-pattern>/unsubscribe</url-pattern>
		
	</servlet-mapping>
	<servlet-mapping>
	
		<servlet-name>controller</servlet-name>
		<url-pattern>/changeSecret</url-pattern>
		
	</servlet-mapping>
	<servlet-mapping>
	
		<servlet-name>controller</servlet-name>
		<url-pattern>/createSecret</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
</web-app>
